<Window x:Class="AluguelImoveis.Views.ImoveisView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Imóveis" Height="300" Width="426">
    <Grid>
        <ListView Name="ImoveisList" Margin="10">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Endereço" DisplayMemberBinding="{Binding Endereco}" Width="200"/>
                    <GridViewColumn Header="Tipo" DisplayMemberBinding="{Binding Tipo}" Width="200"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
